    <string name="menu_RoadmapVoteActivity_result_help_text">Display the current voting results.</string>
    <string name="menu_RoadmapVoteActivity_clear_help_text">All weights will be reset to 0.</string>
    <string name="cab_RoadmapVoteActivity_learn_more_help_text">Learn more about the ticket by reading its page on Github.</string>
    <string name="cab_RoadmapVoteActivity_set_weight_help_text">Select the amount of points that corresponds to how relevant the issue is for you.</string>
